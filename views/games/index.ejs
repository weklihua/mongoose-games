<%- include('../partials/header') %>

<table class="table table-striped table-hover" id="list">
    <thead>
      <tr>
        <th>Title</th>
        <th>Release Date</th>
        <th>Genre</th>
      </tr>
    </thead>
    <tbody>
      <% games.forEach(function(game) { %>
        <tr>
          <td><%= game.title %></td>
          <td><%= game.releaseDate %></td>
          <td><%= game.genre %></td>
          <!-- <td><a href="/movies/<%= game._id %>">DETAILS</a></td> -->
        </tr>
        <% }); %>
      </tbody>
    </table>

<%- include('../partials/footer') %>
